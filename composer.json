{
    "name": "jacques/outsurance-converter",
    "description": "Converts the Outsurance PDF (converted to Excel format with ilovepdf.com) into an Outsurance Excel Spreadsheet (for months Outsurance are incapable of supplying Excel files for)",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "composer/pcre": "^1.0",
        "phpoffice/phpspreadsheet": "^1.20",
        "jacques/php-reports": "^0.0.3"
    },
    "require-dev": {
        "brick/varexporter": "^0.3.5",
        "ergebnis/composer-normalize": "^2.19",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12.9",
        "vimeo/psalm": "^4.16"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Outsurance\\Converters\\": "src/"
        }
    }
}
